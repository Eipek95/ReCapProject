{

  "TokenOptions": {
    "Audience": "engin@engin.com", //projenin web adresi Audience->kitle
    "Issuer": "engin@engin.com", //Issuer Audience uygulayan
    "AccessTokenExpiration": 10, //token geçerlilik süresi dk cinsinden 10 dk
    "SecurityKey": "mysupersecretkeymysupersecretkey" //tokenı oluştururken asp.net kullanacağı anahtar adı.verilen adın uzun olması gerekir
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
